<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:NMGui.Support.XAML.Converters"
                    xmlns:sorting="clr-namespace:NMGui.Support.Sorting">

    <sorting:NaturalSorter x:Key="NaturalSorter" />
    <sorting:GridSortBehaviour x:Key="GridSorting" />

    <!--Converters-Generic-->
    <converters:XAMLStatusColor x:Key="StatusColor" />
    <converters:XAMLStatusText x:Key="StatusText" />
    <converters:BooleanVisibility x:Key="BooleanVisibility" />
    <converters:InverseBooleanVisibility x:Key="InverseBooleanVisibility" />
    <converters:ThicknessConverter x:Key="ThicknessConverter"/>

    <!--Converters-Wizard-->
    <converters:WizardStandardPageOrder x:Key="WizardPageOrder" />
    <converters:WizardPageStringResource x:Key="WizardPageStringResource" />

</ResourceDictionary>